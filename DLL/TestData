CREATE TABLE IF NOT EXISTS public."TestData"
(
    id numeric NOT NULL,
    question text COLLATE pg_catalog."default" NOT NULL,
    answer1 text COLLATE pg_catalog."default" NOT NULL,
    answer2 text COLLATE pg_catalog."default" NOT NULL,
    answer3 text COLLATE pg_catalog."default",
    answer4 text COLLATE pg_catalog."default",
    answer5 text COLLATE pg_catalog."default",
    "rightAnswer" text COLLATE pg_catalog."default" NOT NULL
)